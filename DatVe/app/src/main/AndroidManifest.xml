<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DatVe"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">

        <activity
            android:usesCleartextTraffic="true"
            tools:targetApi="31"
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.datve.LoginActivity" />
        <activity android:name="com.example.datve.RegisterActivity" />
        <activity android:name="com.example.datve.seat.SeatSelectionActivity" />
        <activity android:name="com.example.datve.food.FoodSelectionActivity" />
        <activity android:name="com.example.datve.showtime.SelectShowtimeActivity" />
        <activity android:name="com.example.datve.user.ChangePasswordActivity" />
    </application>

</manifest>